{% extends "../table.html" %}

{% block title %}Pre-Viewer List{% endblock %}

{% block caption %}Pre-Viewer List for {{ bname }}{% endblock %}

{% block thead %}
		<tr>
			<th>nickname</th>
			<th>e-mail</th>
			<th>branch</th>
			<th>player</th>
			<th class="last-column">ID</th>
			<th>accept</th>
			<th class="last-column">reject</th>
		</tr>
{% endblock %}

{% block tbody %}{% for previewer in previewers %}
		{% if not forloop.last %}<tr>{% else %}<tr class="last-row">{% endif %}
			<td>{{ previewer.user.nickname }}</td>
			<td>{{ previewer.user.email }}</td>
			<td>{{ previewer.bid }}</td>
			<td>{{ previewer.pid }}</td>
			<td class="last-column">{{ previewer.key.id }}</td>
			<td><a href="/branch/previewer?action=accept&id={{ previewer.key.id }}">承認</a></td>
			<td><a href="/branch/previewer?action=reject&id={{ previewer.key.id }}">拒否</a></td>
		</tr>{% endfor %}
{% endblock %}
